#include <iostream> 
#include <string>
using namespace std; 
class printData 
{
 public:
       void print(int i) 
      { 
            cout << "Printing int: " << i << endl; 
       } 
       void print(double f) 
       {
            cout << "Printing float: " << f << endl; 
        } 
        void print(string s)
        {
          cout << "Printing string: " << s << endl ;
        }
        void print(char* c) 
        { 
             cout << "Printing character: " << c << endl; //*c 
        } 
        
};      
  

int main(void) 
{ 
        printData pd; // Call print to print integer 
        pd.print(5); // Call print to print float
        pd.print(500.263); // Call print to print character  
        pd.print("hello"); 
        return 0; 
} 
